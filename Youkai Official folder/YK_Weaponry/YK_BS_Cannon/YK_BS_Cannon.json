{
  "Name": "Youkai Noshiro Battleship Cannon",
  "Description": "This long range battleship cannon is stronger than it's Hawkeye counterpart in terms of damage and it's increased ability to rotate, however it has a severe lack of attachment points.",
  "ID": 19049,

  "Faction": 1, 
  "Category": 4, 
  "Grade": 3, 
  "Price": 29763, 
  "HP": 6500, 
  "Mass": 20,

  "Recipe": "47, 47, 47, 54, 54, 54, 54, 55, 55, 55, 55, 61",

  "IconName": "YK_BS_Cannon.png",

  "MeshName": null,

  "MeshMaterialName": "YK_Texture 1.png",
  "MeshGlossTextureName": "YK_Texture 2.png",


  "BlockExtents":
  { "x": 2, "y": 1, "z": 5 },

  "APs": [
    { "x": 0, "y": -0.5, "z": 0},
    { "x": 1, "y": -0.5, "z": 0},
    { "x": 0, "y": -0.5, "z": 1},
    { "x": 1, "y": -0.5, "z": 1},
    { "x": 0, "y": -0.5, "z": 2},
    { "x": 1, "y": -0.5, "z": 2}
  ],

  // Delete /* and */ if using

  //OPTIONAL
  "GamePrefabReference": "HE_CannonTurret_Short_525", //Use a block prefab from the GAME'S RESOURCES as a base. For example, "GSOBlock(111)"
  "KeepReferenceRenderers": false,
  "ReferenceOffset": 
  { "x": 0, "y": 0, "z": 0 },

  "SubObjects": [
    // Replace the original meshes with new ones 
    {
      "ObjectName":                 "m_HE_CannonTurret_Short_Base",
      "MeshName":                   "YK_BS_Cannon_Base.obj",
      "MakeBoxCollider":            true,
      "DestroyExistingRenderer":    true,
      "DestroyExistingColliders":   true,
      "MeshTextureName":            "YK_Texture 1.png",
      "MeshGlossTextureName":       "YK_Texture 2.png", 
      "Position":                   { "x": 1, "y": 0, "z": 0 }
    },
    {
      "ObjectName":                 "m_HE_CannonTurret_Short_Barrel",
      "MeshName":                   "YK_BS_Cannon_Barrel.obj",
      "MakeBoxCollider":            true,
      "DestroyExistingRenderer":    true,
      "DestroyExistingColliders":   true,
      "MeshTextureName":            "YK_Texture 1.png",
      "MeshGlossTextureName":       "YK_Texture 2.png", 
      "Position":                   { "x": 1, "y": 0, "z": 0 }
    },
    {
      "ObjectName":                 "m_HE_CannonTurret_Short_GunMantlet",
      "ColliderMeshName":           "YK_BS_Cannon_Mantle.obj", 
      "MeshName":                   "YK_BS_Cannon_Mantle.obj",
      "MakeBoxCollider":            true,
      "DestroyExistingRenderer":    true,
      "DestroyExistingColliders":   true,
      "MeshTextureName":            "YK_Texture 1.png",
      "MeshGlossTextureName":       "YK_Texture 2.png", 
      "Position":                   { "x": 1, "y": 0, "z": -1 }
    },
    {
      "ObjectName":                 "m_HE_CannonBattleship_216_body",
      "MeshName":                   "YK_BS_Cannon_Body.obj",
      "MakeBoxCollider":            true,
      "DestroyExistingRenderer":    true,
      "DestroyExistingColliders":   true,
      "MeshTextureName":            "YK_Texture 1.png",
      "MeshGlossTextureName":       "YK_Texture 2.png", 
      "Position":                   { "x": 1, "y": 0.5, "z": -0.5 }
    },
    // Move the other components to their proper places
    {
      "ObjectName":                 "_gimbalBase/_gimbalElev/_barrel/_smoke",
      "Position":                   { "x": 0.5, "y": 0, "z": 0 }
    },   
    {
      "ObjectName":                 "_gimbalBase/_gimbalElev/_barrel/_muzzleFlash",
      "Position":                   { "x": 0, "y": 0, "z": 0 },
      "Scale":                      { "x": 0, "y": 0, "z": 0 }
    },
    {
      "ObjectName":                 "_gimbalBase/_gimbalElev/_barrel/_recoiler",
      "Scale":                      { "x": 1, "y": 1, "z": 1 }
    }, 
    {
      "ObjectName":                 "_gimbalBase/_gimbalElev/_barrel/_casingSpawn",
      "Position":                   { "x": 0.5, "y": -0.03, "z": -1.5 }
    },    
    {
      "ObjectName":                 "_gimbalBase/_gimbalElev/_barrel/_bulletSpawn",
      "Position":                   { "x": 0.5, "y": 0.1, "z": 3.5 }
    }  
  ],

  "Deserializer":{
    "GameObject|_gimbalBase": {
      "UnityEngine.Transform":    { "localPosition":  { "x": 0, "y": -0.5, "z": 0.5 } },
      "GameObject|m_HE_CannonBattleship_216_body": {

      },
      "GimbalAimer": {
        "rotationAxis": 2,
        "rotationLimits": [
          15.0,
          -15.0
        ]
      },
      "GameObject|_gimbalElev": {
        "UnityEngine.Transform":    { "localPosition":  { "x": 0, "y": 0.5, "z": 0.5 } },
        "GimbalAimer": {
          "rotationAxis": 1,
          "rotationLimits": [
            65.0,
            -8.5
          ]
        },
        "GameObject|m_HE_CannonTurret_Short_GunMantlet": {}
      }
    },

    "ModuleWeapon": { //Component
      "m_RotateSpeed" : 20.0 //SPEED of turret aiming, not homing
    },
	"ModuleWeaponGun": {
      "m_ShotCooldown": 2.5
    },
    "FireData": { //Component, but with a catch
      "m_MuzzleVelocity": 160, //Bullet speed
      "m_BulletSprayVariance": 0.015, //Bullet inverse-accuracy
      "m_KickbackStrength": 750, // Would you like to become a plane
      "Instantiate|m_BulletPrefab": {
        "SmokeTrail":{
          "numberOfPoints":   10.0,   // Dictates the length of the trail by telling the lineRenderer how many points to create while being spawned.
          "spread":           0.0,   // Dictates how erratic the trail is.
          "riseSpeed":        0.1,   // Dictates how fast the trail rises. This will make the trail act more like a smoke
          "updateSpeed":      0.15,   // DIctates the speed of the trail.
      },
      "LineRenderer": {
        "startWidth": 0.5,
        "endWidth": 0.1,
        "widthMultiplier": 1.0,
        "startColor": {
            "r": 1.0,
            "g": 0.0,
            "b": 0.3,
            "a": 1.0
        },
        "endColor":{
            "r": 1.0,
            "g": 0.1,
            "b": 0.2,
            "a": 1.0
        }
      }, //Makes bullet unique, enters like "GameObject|m_BulletPrefab"
        "UnityEngine.Transform": {"localScale": { "x":1.5, "y":1.5, "z":1.8 }},  // Size of Bullet    
        "WeaponRound": { //Component because we are in GameObject right now
          "m_DamageType" : "3",
          "m_Damage": 3500 //This does nothing apparently, so you can probably just delete this block
        },
        "Projectile": { //Another one (doot doooooot dododoot)
          "m_ImpactSFXType" : "Cannon", //Makes for more intense hit noises
          "Instantiate|m_Explosion": { //Makes explosion unique, enters like "GameObject|m_Explosion"
            "Explosion": { //wOW a component
              "m_EffectRadius": 8.0,
              "m_EffectRadiusMaxStrength": 6.0,
              "m_MaxDamageStrength": 250.0,
              "m_MaxImpulseStrength": 250.0,
              "m_DamageType": 3,
              "m_ExplosionType": 0,
              "m_ExplosionSize": 1
            }
          }
        }
      }
    }
  }
}
