    "GameObject|_gimbalBase": {
      "UnityEngine.Transform":    { "localPosition":  { "x": 2, "y": -0.05, "z": 2 } },
      "GameObject|_gimbalElev": {
        "UnityEngine.Transform":  { "localPosition": { "x": 0, "y": 0.2, "z": -0.2 } },
        "GimbalAimer":            { "rotationLimits": [-15, 40 ] },
        "GameObject|_barrel_L": {
          "UnityEngine.Transform": { "localPosition": { "x": -0.50, "y": 0, "z": 0 } }},
        "GameObject|_barrel_R": {
          "UnityEngine.Transform": { "localPosition": { "x": 0.50, "y": 0, "z": 0 } }}       
      }
    },
    //********** Module Weapon **********//
    "ModuleWeapon": { 
      "m_FireSFXType": 64, 
      "m_RotateSpeed": 45.0 
    },
    "ModuleWeaponGun": {
      "m_FireControlMode": 0,
      "m_ShotCooldown" : 0.85, 
	    "m_CooldownVariancePct": 0.0,
      //"m_BurstShotCount": 4,
      //"m_BurstCooldown": 2.90,
      "m_ResetBurstOnInterrupt": false,
      "m_NumCannonBarrels": 2
    },
    "FireData": {
      "m_MuzzleVelocity":         95,        // Dictates the projectile's speed. The higher the value, the faster it will travel.
      "m_KickbackStrength":       650,        // Dictates the strength of the recoil when firing the gun
      "m_BulletSprayVariance":    0.05,      // Dictates how accurate the gun is. The lower the value, the more accurate it fires its shots.
      "m_CasingVelocity": 10.0,                // Dictates the speed in which the casing ejects
      "m_BulletSpin": 0.0,                    // Dictates the rate of the bullet spinning in mid-air. Set to 0 by default
      "m_CasingEjectVariance": 0.3,           // Dictates the randomness of the casing's ejection. A bit of of value here will make the casing ejection more visually interesting
      "m_CasingEjectSpin": 50.0,              // Dictates the amount of spin done by the casing when ejected. A bit of of value here will make the casing ejection more visually interesting
      "Instantiate|m_BulletPrefab": { 
        "UnityEngine.Transform": {
          "localPosition": { "x":0, "y":0, "z":0 },
          "localScale": { "x":1.5, "y":1.5, "z":1.5 }
        },
        "Projectile": {
          "Instantiate|m_Explosion": {
            "Explosion": {
                "m_ExplosionType": 3,                   // Dictates the damage type of the explosion
                "m_EffectRadius": 4,                    // Dictates the explosion's damage radius. The HE MK3 Battleship cannon's radius is 10.
                "m_MaxDamageStrength":      750.0,      // The splash damage done
                "m_MaxImpulseStrength":     700.0,      // The strength of the knockback done by the explosion
                "m_EffectRadiusMaxStrength": 1.5, 
                "m_DamageType": 3
            }
          }
        },
        "WeaponRound": { 
          "m_Damage": 1250
        },
        "SmokeTrail":{
          "numberOfPoints":   10.0,   // Dictates the length of the trail by telling the lineRenderer how many points to create while being spawned.
          "spread":           0.0,   // Dictates how erratic the trail is.
          "riseSpeed":        0.1,   // Dictates how fast the trail rises. This will make the trail act more like a smoke
          "updateSpeed":      0.15,   // DIctates the speed of the trail.
      },
      "LineRenderer": {
        "startWidth": 0.5,
        "endWidth": 0.1,
        "widthMultiplier": 1.0,
        "startColor": {
            "r": 1.0,
            "g": 0.0,
            "b": 0.3,
            "a": 1.0
        },
        "endColor":{
            "r": 1.0,
            "g": 0.1,
            "b": 0.2,
            "a": 1.0
        }
