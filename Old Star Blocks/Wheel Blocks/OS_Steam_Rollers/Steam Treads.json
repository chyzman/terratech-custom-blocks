      "GameObject|m_GC_Tracks_238_Wheel_01": { //model of wheel
        "UnityEngine.Transform": {
          "localScale": {
            "x": 1,
            "y": 0.9,
            "z": 0.9
          }
        },
        //We house the other aux wheels in here because this one also controls the track's belt animation
        "GameObject|m_AuxWheel_02": { //model of wheel
          "UnityEngine.Transform": {
            "localScale": {
              "x": 1,
              "y": 1,
              "z": 1
            }
          },
          "FollowTransform": { //snap to the b_Aux bones
            "m_TransformToFollow": "/b_Aux_02",
            "m_FollowOrientation": false
          },


          // follow code-------------------------------------------------------
          "GameObject|Suspension_Up": {
            // As Suspension_Up is supposed to determine the z-axis upright rotation of FollowSuspensions assigned to it, it's genrerally the safest case to leave it in the base GameObject.
            "UnityEngine.Transform": {
              "localEulerAngles": {
                "x": 0,
                "y": 0,
                "z": 0
              },
              "localPosition": {
                "x": 0,
                "y": 3,
                "z": 0
              },
              "localScale": {
                "x": 1,
                "y": 1,
                "z": 1
              }
            }
          },
          "GameObject|m_AuxWheel_02": {
            "GameObject|Pv_piston2": { //Wheel to Base
              "GameObject|PistonThin": { // Thin on the wheel
                "UnityEngine.Transform": { //
                  "localPosition": { //
                    "x": 0.0,
                    "y": 0.0,
                    "z": 0.0
                  },
                  "localEulerAngles": {
                    "x": 0,
                    "y": 0,
                    "z": 0
                  }
                },
                "FollowSuspension": { // (set this to point at the fixed base)
                  "m_TargetWheelTrans": "/PistonPivot/Pv_piston1",
                  "m_TargetUpTrans": "/Suspension_Up",
                  "m_KeepOriginalRotationOffset": false,
                  "m_ScaleToMatchDistance": false
                },
                "GameObject|Piston_B_Bone": { //(placed this within "Piston_A" to follow the orientation of the parent GameObject [Piston_A])
                  "FollowTransform": { // FollowTransform provides the "anchor point" around which the Piston can rotate.
                    //It can also be used to fasten this GameObject's center to somewhere else on the model and keep the rotation of this GameObject's originatingParent.
                    //  to do this, just keep "m_FollowOrientation": false,
                    "m_TransformToFollow": "/PistonPivot/Pv_piston1", //This path determines the "anchor point". Pv_piston1 can rotate around this point, but the distance stays fixed.
                    "m_FollowOrientation": false
                  },
                  "GameObject|PistonThick": { //Actual Model is located here (adjusted)
                    "UnityEngine.Transform": {
                      "localEulerAngles": {
                        "x": 0, //Get it to aim back at the other half of the piston-
                        "y": 0,
                        "z": 0
                      }
                    }
                  }
                }
              }
            }
          }

        }, // end of follow code   
        "GameObject|m_AuxWheel_01": { //model of wheel
          "UnityEngine.Transform": {
            "localScale": {
              "x": 1,
              "y": 0.5,
              "z": 0.5
            }
          },
          "FollowTransform": { //snap to the b_Aux bones
            "m_TransformToFollow": "/b_Aux_01",
            "m_FollowOrientation": false
          }
        }
      }
