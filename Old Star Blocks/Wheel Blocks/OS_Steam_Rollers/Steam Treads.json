    "GameObject|PistonPivot": { // PistonPivot is the target "m_AuxWheel_02" provides the movement
      "FollowTransform": {
        "m_TransformToFollow": "/m_AuxWheel_02", //Very center of the wheel
        "m_FollowOrientation": true //Remain aligned with the root
      },
      "GameObject|m_AuxWheel_02": { //The actual model is here, offsetting the model from the very center of the wheel
        "UnityEngine.Transform": {
          "localEulerAngles": {
            "x": 0,
            "y": 0,
            "z": 0
          }, //Controls rotation relative to parent
